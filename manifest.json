{
  "name": "Linkedin AutoConnect",
  "description": "Sends automatic connection request in linkedin to specific people",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://www.linkedin.com/", "https://inkedin.com/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}
